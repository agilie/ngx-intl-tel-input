{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"downlevelIteration": true,
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2018",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom",
			"es2017",
			"es2015"
		],
		"paths": {
			"ngx-intl-tel-input": [
				"dist/ngx-intl-tel-input"
			],
			"ngx-intl-tel-input/*": [
				"dist/ngx-intl-tel-input/*"
			]
		}
	}
}
